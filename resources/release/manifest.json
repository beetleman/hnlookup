{
  "name": "HNhit",
  "version": "0.1.0",
  "browser_action": {
    "default_title": "Show Hacker News hits",
    "default_popup": "popup.html",
    "default_icon": {
      "19": "images/icon19.png",
      "38": "images/icon38.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "permissions": [
    "tabs",
    "https://hn.algolia.com"
  ],
  "manifest_version": 2
}
